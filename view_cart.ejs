<script src="https://checkout.stripe.com/checkout.js"></script>
<script>

var total_amount_before_discount = 0;

var discount_amount = 0;

var discount_percentage = 0;

var delivery_amount = 100;

var discount_valid_amount = 0;

var final_total_amount = 0;



/////////////////Stripe JS Code Start////////////////

  document.getElementsByClassName('btn-purchase')[0].addEventListener('click', function(){

    var input_address = document.getElementById("input_address").value;
      if(input_address.trim() !== '') {

        purchaseClicked();

      }
      else{

        document.getElementById("address_alert").innerHTML = "<div class='alert alert-danger alert-dismissible fade show' role='alert'>Enter address<button type='button' class='btn-close' data-bs-dismiss='alert' aria-label='Close'></button></div>";
      }

  })

var stripeHandler  =  StripeCheckout.configure({
    key: stripePublicKey,
    locale: 'en',
    token: function(token) {

      var input_address = document.getElementById("input_address").value;
     var input_other_description = document.getElementById("input_other_description").value;

       fetch('/create-payment-intent',  {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json',
                'Accept': 'application/json'
            },
            body: JSON.stringify({
                stripeTokenId: token.id, 
                address: input_address, other_description: input_other_description, payable_amount: final_total_amount,
                 }),

            data: JSON.stringify({  address: input_address, other_description: input_other_description, payable_amount: final_total_amount, }),
        }).then(function(res) {


        
      

      return res.json(); 
        
        }).then(function(data) {

          document.getElementById("show_order_number").innerHTML = data.order_number
          document.getElementById("order_confirmation").style.display = "block"; 
          document.getElementById("orders_table").style.display = "none"; 
          document.getElementById("coupon_section").style.display = "none"; 
          document.getElementById("address_section").style.display = "none"; 
          document.getElementById("anyotherdesc_section").style.display = "none";
          document.getElementById("payment_section").style.display = "none";     
          document.getElementById("checkout_btn").disabled = true;
          //  alert(data.message)
           

        }).catch(function(error) {

          document.getElementById("address_alert").innerHTML = "<div class='alert alert-danger alert-dismissible fade show' role='alert'>Failed<button type='button' class='btn-close' data-bs-dismiss='alert' aria-label='Close'></button></div>";
 
        })
    }
})

function purchaseClicked() {
   
  stripeHandler.open();

}





/////////////////Stripe JS Code End////////////////



document.getElementById("clear_cart").addEventListener('click',function(){

  const xhr = new XMLHttpRequest();
  xhr.open('POST', '/clear_cart');
  xhr.setRequestHeader('Content-Type', 'application/json');
  xhr.onload = function() {
    if (xhr.status === 200) {

      show_cart();

    }
    if(xhr.status === 404){
      alert("not found");
    }
  
  }
  xhr.send();

});


document.getElementById("checkout_btn").addEventListener('click',function(){

  
  var input_address = document.getElementById("input_address").value;
  var input_other_description = document.getElementById("input_other_description").value;
  var radiocashdelivery = document.getElementById("radiocashdelivery");

  if(input_address.trim() !=="" && radiocashdelivery.checked == true){
    
    
    $.ajax({
      type: 'POST',
      url: '/place_order_cash_on_delivery',
      data: JSON.stringify({ address: input_address, other_description: input_other_description,payable_amount:final_total_amount }),
      contentType: 'application/json',
	  success: function(response) {
     
     
      //alert(response.message);

      document.getElementById("show_order_number").innerHTML = response.order_number

      document.getElementById("order_confirmation").style.display = "block"; 

document.getElementById("orders_table").style.display = "none"; 

document.getElementById("coupon_section").style.display = "none"; 

document.getElementById("address_section").style.display = "none"; 

document.getElementById("anyotherdesc_section").style.display = "none";

document.getElementById("payment_section").style.display = "none"; 
     


     document.getElementById("checkout_btn").disabled = true;

  


	},

      error: function() {
        alert('Failed to update item quantity');
      }
    });



  }
  else{
  document.getElementById("address_alert").innerHTML = "<div class='alert alert-danger alert-dismissible fade show' role='alert'>Enter address<button type='button' class='btn-close' data-bs-dismiss='alert' aria-label='Close'></button></div>";

  }
});


  
  function removeFromCart(item_id){
    	const xhr = new XMLHttpRequest();
		xhr.open('POST', '/remove-to-cart');
		xhr.setRequestHeader('Content-Type', 'application/json');
		xhr.onload = function() {
			if (xhr.status === 200) {
			// Confirm the item has been added to the cart
        show_cart();
			} else {
			// Handle the error
			alert("Failed to remove item to cart");
			}
		};
		xhr.send(JSON.stringify({ itemId: item_id }));
}




function show_cart() {
	
  total_amount_before_discount = 0;
	
  const xhr = new XMLHttpRequest();
  xhr.open('GET', '/cart');
  xhr.setRequestHeader('Content-Type', 'application/json');
  xhr.onload = function() {
    if (xhr.status === 200) {
      const cart = JSON.parse(xhr.responseText);
	 
	  document.getElementById("cart_items").innerHTML = "";
	 
    

    if(Object.keys(cart.items).length === 0){
      
 
    document.getElementById("cart_items").innerHTML = "<tr> <th scope='row' style='color:red'>No item in the cart</th> <tr>" 
      document.getElementById("coupon_section").style.display="none";
      document.getElementById("address_section").style.display="none";
      document.getElementById("anyotherdesc_section").style.display="none";
      document.getElementById("payment_section").style.display="none";
      document.getElementById("clear_cart").style.display="none";
      

    }

		for (let index in cart.items) {
	
     

			total_amount_before_discount += parseInt(cart.items[index][1])*parseInt(cart.items[index][0]);

			console.log(index +" - "+cart.items[index]);
	//	document.getElementById("cart_items").innerHTML +="<div class='cart-item'  ><div class='item-num'>"+i+"</div><div class='item-details'><h4>"+cart.items[index][2]+"</h4><div class='item-qty'>Qty: <div class='cart-item-qty'  data-item-id='"+index+"'  style='display:inline-flex;'><button class='qty-btn' data-operation='minus'>-</button> "+cart.items[index][0]+"<button class='qty-btn' id='add_qty"+index+"' data-operation='plus'>+</button></div></div></div><div class='item-price'>"+parseInt(cart.items[index][1])*parseInt(cart.items[index][0])+"</div><button data-item-id='"+index+"'  class='remove-btn'>&times;</button> </div>";
	
			
    document.getElementById("cart_items").innerHTML += `
    <tr>
    <th scope='row'>
      <div class='d-flex align-items-center'>
        <img src='images/product/`+index+`.png' class='img-fluid rounded-3'
          style='width: 120px;' alt='Book'>
        <div class='flex-column ms-4'>
          <p class='mb-2'>`+cart.items[index][2]+`</p>
          
        </div>
      </div>
    </th>
   
    <td class='align-middle item-qty'>
      <div class='d-flex flex-row cart-item-qty'  data-item-id='`+index+`' >
        <button class='btn btn-link px-2 qty-btn' id='minus_qty`+index+`' data-operation='minus'>
          <i class='fa fa-minus'></i>
        </button>

        <input id='form1' min='1' name='quantity' value='`+cart.items[index][0]+`' type='number'
          class='form-control form-control-sm' id='qty_number_Input' style='width: 50px;' onchange='validate_qty_number_Input(this,`+index+`)'  />

        <button class='btn btn-link px-2 qty-btn' id='add_qty`+index+`' data-operation='plus'>
          <i class='fa fa-plus'></i>
        </button>
      </div>
    </td>
    <td class='align-middle'>
      <p class='mb-0' style='font-weight: 500;'>`+parseInt(cart.items[index][1])*parseInt(cart.items[index][0])+`</p>
    </td>
    <td class='align-middle'><button data-item-id=`+index+` class='btn-danger remove-btn'>&times;</button></td>
  </tr>
    `;

		
    check_qty_limit(index, function(availableQty) {
					if(cart.items[index][0] ==availableQty){
						document.getElementById("add_qty"+index).setAttribute("disabled","disabled");
					}

          if(cart.items[index][0] == 1){
						document.getElementById("minus_qty"+index).setAttribute("disabled","disabled");
					}


			});

		

		}

document.getElementById("total_price").innerHTML = total_amount_before_discount;

if(total_amount_before_discount < discount_valid_amount){
  chk();
}

discount_amount = (total_amount_before_discount * discount_percentage/100);

document.getElementById("Coupon_amount").innerHTML = "-"+discount_amount;

document.getElementById("delivery_amount").innerHTML = "+"+delivery_amount;

final_total_amount = total_amount_before_discount + delivery_amount - discount_amount;

document.getElementById("final_total_amount").innerHTML = final_total_amount;



      // Do something with the cart data
    } else {
      alert("Failed to get cart data");
    }
  };
  xhr.send();
}

$(document).on('click', '.remove-btn', function() {
    var item_id = $(this).data('item-id');

    removeFromCart(item_id);
    
});



function update_cart(itemId,operation){
      // Send a request to update the item quantity
      $.ajax({
      type: 'POST',
      url: '/update-cart-quantity',
      data: JSON.stringify({ itemId: itemId, operation: operation }),
      contentType: 'application/json',
	  success: function(response) {
			// Reload the cart items after the quantity has been updated
			show_cart();
			

	},

      error: function() {
        alert('Failed to update item quantity');
      }
    });
}


$(document).on('click', '.cart-item-qty button', function() {

var operation = $(this).data('operation');
  var itemId = $(this).closest('.cart-item-qty').data('item-id');

 
  update_cart(itemId,operation);
  
});



function check_qty_limit(index, callback){
  $.ajax({
    type: 'POST',
    url: '/check-qty-limit',
    data: JSON.stringify({ itemId: index }),
    contentType: 'application/json',
    success: function(response) {
      // Call the callback function with the availableQty
      callback(String(response.availableQty));
    },
    error: function() {
      console.log('Failed to check item quantity');
      // Call the callback function with a default value or error message
      callback(null);
    }
  });
}



function chk(){
  $.ajax({
                method:"GET",
                url:"/remove_coupon",
               
                success:function(result)
                {

           
                  document.getElementById("applied_coupon").innerHTML ="";
                  document.getElementById("input_coupon").value="";

                  discount_amount = 0;
                  discount_valid_amount = 0;

                  document.getElementById("Coupon_amount").innerHTML = "-"+discount_amount;

                  final_total_amount = total_amount_before_discount + delivery_amount - discount_amount;

                  document.getElementById("final_total_amount").innerHTML = final_total_amount;

                
                },
                error: function(jqXHR,textStatus,errorThrow){

                      console.log("Server Error")

                  }    
            });
         
}


function check_attach_coupon(){


  $.ajax({
                method:"GET",
                url:"/check_attach_coupon",
              
                success:function(result)
                {
                  // `<p><small style="float: left;padding-bottom: 1%;">`+result+`</small></p>`

             if(result.applied_coupon.length > 0){

               
                    document.getElementById("applied_coupon").innerHTML = `<div class='card'>
  <strong>Code : `+result.applied_coupon[1]+`&nbsp;&nbsp; <a href="#" onclick='chk()'><i class='fa fa-times'  id='remove_coupon' style='color:red'></i></a> <br> Discount : `+result.applied_coupon[2]+`% <br> Valid at : `+result.applied_coupon[3]+`  </strong>
</div>`;


discount_percentage = parseInt(result.applied_coupon[2]);

discount_amount = (total_amount_before_discount * discount_percentage/100);

discount_valid_amount = result.applied_coupon[5];

document.getElementById("Coupon_amount").innerHTML = "-"+discount_amount;

final_total_amount = total_amount_before_discount + delivery_amount - discount_amount;

document.getElementById("final_total_amount").innerHTML = final_total_amount;


             }

                },
                error: function(jqXHR,textStatus,errorThrow){
                    document.getElementById("applied_coupon").innerHTML = `<div>
                        <strong style='color:red'>`+jqXHR.responseText+`</strong>
                      </div>`;

                      discount_amount= 0;
                      discount_valid_amount = result.applied_coupon[5];
                      document.getElementById("Coupon_amount").innerHTML = "-"+discount_amount
                }    
            })

}

check_attach_coupon();

  $(document).ready(function () {

    
          $("#couponform").submit(function (event) {
            event.preventDefault();
            const input_coupon = $("#input_coupon").val();
            const subtotal = total_amount_before_discount;
           
            $.ajax({
                method:"POST",
                url:"/verify_coupon",
                data: {input_coupon:input_coupon,subtotal:subtotal},
                success:function(result)
                {
             
                    document.getElementById("showAlert").innerHTML = `<div class='alert alert-success alert-dismissible fade show' role='alert'>`+result+`<button type='button' class='btn-close' data-bs-dismiss='alert' aria-label='Close'></button></div>`;

                    document.getElementById("input_coupon").value="";
                    
check_attach_coupon();

                },
                error: function(jqXHR,textStatus,errorThrow){
                    document.getElementById("showAlert").innerHTML =`<div class='alert alert-danger alert-dismissible fade show' role='alert'>`+jqXHR.responseText+`<button type='button' class='btn-close' data-bs-dismiss='alert' aria-label='Close'></button></div>`
               
                  }    
            })
          });
        });


function validate_qty_number_Input(inputElement,itemId) {
  if (inputElement) {
    var inputValue = inputElement.value;
    
    if (inputValue <= 0) {
      inputElement.value = "1";
    }

    update_by_input_cart(itemId,inputElement.value);
    
  }
}




function update_by_input_cart(itemId,value){
      // Send a request to update the item quantity
      $.ajax({
      type: 'POST',
      url: '/update_qty_by_input_cart',
      data: JSON.stringify({ itemId: itemId, value: value }),
      contentType: 'application/json',
	  success: function(response) {
			// Reload the cart items after the quantity has been updated
			show_cart();
			

	},

      error: function() {
        alert('Failed to update item quantity');
      }
    });
}


show_cart();


</script>


